<%= form_with(model: admin_parking, class: "form") do |form| %>
  <% if admin_parking.errors.any? %>
    <div class="alert alert-danger">
      <h2><%= pluralize(admin_parking.errors.count, "error") %> prohibited this admin_parking from being saved:</h2>

      <ul>
        <% admin_parking.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= form.label :vehicle_id, class: "control-label" %>
    <%= form.number_field :vehicle_id, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :vehicle_type_id, class: "control-label" %>
    <%= form.select :vehicle_type_id, Admin::VehicleType.all.collect {|t| [t.name, t.id]}, {}, {class: 'form-control'} %> 
   </div>

  <div class="form-group">
    <%= form.label :parking_lot_id, class: "control-label" %>
    <%= form.select :parking_lot_id, Admin::ParkingLot.all.collect {|t| [t.name, t.id]}, {}, {class: 'form-control'} %>
  </div>

  <div class="form-group">
    <%= form.submit "Save", class: "btn btn-primary" %>
  </div>
<% end %>

